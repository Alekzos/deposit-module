{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6" //Добавил в соответсвении с инструкцией MUI https://mui.com/guides/typescript/
    ],

    //Добавил в соответсвении с инструкции MUI https://mui.com/guides/typescript/ чтобы совместить с TS
    "noImplicitAny": true, 
    "noImplicitThis": false,
    "strictNullChecks": true,

    //для материала
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
